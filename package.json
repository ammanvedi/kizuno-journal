{
  "name": "kizuno-journal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clerk-middleware-tunnel": "ngrok http --domain=faithful-welcomed-gannet.ngrok-free.app 3000",
    "dev:db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "dev:db:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prod:db:deploy": "npx prisma migrate deploy",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "build": "sh vercel.sh",
    "build:production": "prisma generate && npm run prod:db:deploy && next build",
    "build:preview": "next build"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.2.4",
    "@prisma/client": "^5.17.0",
    "dotenv-cli": "^7.4.2",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "localtunnel": "^2.0.2",
    "prisma": "^5.17.0",
    "typescript": "^5.5.4"
  }
}
